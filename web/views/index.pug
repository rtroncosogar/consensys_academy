    script(type="application/javascript").
      var checkerInterval = setInterval(function() {
        console.log("Checking for authorization at #{linkToken}");
        $.get("amiauthorized?token=#{linkToken}", function(data, status){
          if(status === "success") {
            if(data.status === "Authorized") {
              // redirect to login
              console.log("Authorized");
              clearInterval(checkerInterval);
              window.location.href = '/login?token=#{linkToken}';
            } 
            // else do nothing
            console.log("Unauthorized");
          }
        });
      }, 1000);
